        {
          "datasource": {
            "type": "postgres",
            "uid": "P44368ADAD746BC27"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  \"colonyId\" AS \"Colony\",\n  \"speciesId\" AS \"Species\",\n  AVG(lat) * (180 / PI()) AS \"Latitude\",\n  AVG(lon) * (180 / PI()) AS \"Longitude\"\nFROM\n  (\n    SELECT\n      \"colonyId\",\n      \"speciesId\",\n      ATAN2(\n        6371000 * SIN(latitude * (PI() / 180)),\n        sqrt(\n          (\n            AVG(\n              6371000 * COS(latitude * (PI() / 180)) * COS(longitude * (PI() / 180))\n            ) * AVG(\n              6371000 * COS(latitude * (PI() / 180)) * COS(longitude * (PI() / 180))\n            )\n          ) + (\n            AVG(\n              6371000 * COS(latitude * (PI() / 180)) * SIN(longitude * (PI() / 180))\n            ) * AVG(\n              6371000 * COS(latitude * (PI() / 180)) * SIN(longitude * (PI() / 180))\n            )\n          )\n        )\n      ) AS lat,\n      ATAN2(\n        AVG(\n          6371000 * COS(latitude * (PI() / 180)) * SIN(longitude * (PI() / 180))\n        ),\n        AVG(\n          6371000 * COS(latitude * (PI() / 180)) * COS(longitude * (PI() / 180))\n        )\n      ) AS lon\n    FROM\n      gps_track\n      JOIN bird ON bird.id = \"birdId\"\n    WHERE\n      gps_track.\"isDive0m\" IS FALSE\n      AND gps_track.\"isDive1m\" IS FALSE\n      AND gps_track.\"isDive2m\" IS FALSE\n      AND gps_track.\"isDive3m\" IS FALSE\n      AND gps_track.\"isDive4m\" IS FALSE\n      AND gps_track.\"isDive5m\" IS FALSE\n    GROUP BY\n      \"colonyId\",\n      \"speciesId\",\n      latitude,\n      longitude\n  ) AS AVG\nGROUP BY\n  \"colonyId\",\n  \"speciesId\"",
          "refId": "Centroid",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Centroid of Coordinates",
      "type": "geomap"
